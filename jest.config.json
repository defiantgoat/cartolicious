{
  "transform": {
    ".(ts|tsx)": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "moduleNameMapper": {
    ".+\\.(css|scss)$": "identity-obj-proxy"
  },
  "collectCoverage": true,
  "coverageDirectory": "./coverage",
  "collectCoverageFrom": [
    "src/**/*.(ts|tsx|js)",
    "!src/index.tsx",
    "!src/tests/*"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 95,
      "functions": 100,
      "lines": 95
    }
  },
  "globals": {
    "ts-jest": {
      "diagnostics": {
        "warnOnly": true
      }
    }
  }
}